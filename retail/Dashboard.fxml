<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" prefHeight="400.0" prefWidth="600.0" spacing="15" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="retail.DashboardController">
    <children>
        
        <Button onAction="#openProduct" prefHeight="34.0" prefWidth="300.0" text="Product Management" />
        <Button onAction="#openCustomer" prefHeight="34.0" prefWidth="200" text="Customer Management" />
        <Button onAction="#openBuyProducts" prefHeight="34.0" prefWidth="300.0" text="Buy Products" />
        <Button onAction="#openBillingView" prefHeight="34.0" prefWidth="200" text="View All Bills" />
        <Button onAction="#openInventory" prefHeight="34.0" prefWidth="300.0" text="Inventory Management" />
        <Button onAction="#handleLogout" prefHeight="34.0" prefWidth="200.0" text="Logout" />
    </children>
</VBox>