<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400" prefWidth="400" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="retail.InventoryController">
    <children>
        <TableView fx:id="tblInventory" prefHeight="420.0" prefWidth="620.0">
            <columns>
                <TableColumn fx:id="colId" prefWidth="100.0" text="ID" />
                <TableColumn fx:id="colName" prefWidth="350.0" text="Product Name" />
                <TableColumn fx:id="colQuantity" prefWidth="150.0" text="Quantity" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
